{
    "manifest_version": 3,
    "name": "AI & Automation News",
    "version": "1.1",
    "description": "Latest headlines about AI and Automation.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["storage"],
    "host_permissions": [
      "https://news.google.com/*"
    ]
  }
  